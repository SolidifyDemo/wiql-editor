{
    "manifestVersion": 1,
    "id": "wiql-search",
    "version": "0.1.117",
    "name": "Wiql Search",
    "scopes": [ "vso.work", "vso.work_write" ],
    "description": "Search work items with wiql queries.",
    "publisher": "ottostreifel",
    "icons": {
        "default": "img/logo.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "Sample"
    ],
    "content": {
        "details": {
            "path": "details.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://bit.ly"
        },
        "getstarted": {
            "uri": "https://bit.ly"
        },
        "learn": {
            "uri": "https://bit.ly"
        },
        "support": {
            "uri": "https://bit.ly"
        },
        "repository": {
            "uri": "https://bit.ly"
        },
        "issues": {
            "uri": "https://bit.ly"
        }
    },
    "branding": {
        "color": "rgb(220, 235, 252)",
        "theme": "light"
    },
    "files": [
        {
            "path": "img",
            "addressable": true
        },
        {
            "path": "build",
            "addressable": true
        },
        {
            "path": "index.html",
            "addressable": true
        }
    ],
    "categories": [
        "Integrate"
    ],
    "contributions": [
        {
            "id": "wiql-search-hub",
            "type": "ms.vss-web.hub",
            "description": "Hub for wiql queries",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Wiql Search",
                "uri": "index.html"
            }
        }
    ]
}
